    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^48.0.1"
  },
  "devDependencies": {
    "@squidfunk/mono-transform": "*"
  },
  "peerDependencies": {
    "eslint": "8.x"
  }
