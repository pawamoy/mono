  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/squidfunk/mono.git",
    "directory": "presets/eslint-config"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "files": [
    "./dist",
    "./typings"
  ],
  "scripts": {
    "build": "tsc",
    "check": "run-p check:*",
