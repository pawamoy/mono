    "stylelint-scss": "^5.3.1",
    "stylelint-stylistic": "^0.4.3"
  },
  "devDependencies": {
    "@squidfunk/mono-resolve": "^0.1.5",
    "@squidfunk/mono-transform": "^0.1.5",
    "stylelint": "^16.0.1"
  },
  "peerDependencies": {
    "stylelint": "15.x"
  }
